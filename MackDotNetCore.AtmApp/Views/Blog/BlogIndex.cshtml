@model List<BlogDataModel>
@{
	ViewData["Title"] = "Blog Index";
	int count = 0;
}
<h4>User Lists </h4>
<table class="table">
	<thead>
		<tr>
			<th scope="col">#</th>
			<th scope="col">BlogId</th>
			<th scope="col">CardNumber</th>
			<th scope="col">Pin</th>
			<th scope="col">FirstName</th>
			<th scope="col">LastName</th>
			<th scope="col">Balance</th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model)
		{
			<tr>
				<th scope="row">@(++count)</th>
				<td>@item.BlogId</td>
				<td>@item.CardNum</td>
				<td>@item.Pin</td>
				<td>@item.FirstName</td>
				<td>@item.LastName</td>
				<td>@item.Balance</td>
			</tr>

		}
	</tbody>

</table>